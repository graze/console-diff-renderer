{
    "name": "graze/console-diff-renderer",
    "description": "only write output to a console that has changed",
    "keywords": [
        "graze",
        "console-diff-renderer"
    ],
    "config": {
        "platform": {
            "php": "8.1"
        }
    },
    "homepage": "https://github.com/graze/console-diff-renderer",
    "license": "MIT",
    "authors": [
        {
            "name": "Harry Bragg",
            "email": "harry.bragg@graze.com",
            "role": "Developer"
        },
        {
            "name": "Graze Developers",
            "email": "developers@graze.com",
            "homepage": "http://www.graze.com",
            "role": "Development Team"
        }
    ],
    "require": {
        "php": "^5.6 | ^7.0 | ^8.0",
        "symfony/console": "^2.8 | ^3.2 | ^4.0 | ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.21 | ^6 | ^7",
        "squizlabs/php_codesniffer": "^3",
        "graze/standards": "^2.0",
        "mockery/mockery": "^1"
    },
    "autoload": {
        "psr-4": {
            "Graze\\DiffRenderer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Graze\\DiffRenderer\\Test\\": "tests/src",
            "Graze\\DiffRenderer\\Test\\Unit\\": "tests/unit",
            "Graze\\DiffRenderer\\Test\\Integration\\": "tests/integration"
        }
    }
}
